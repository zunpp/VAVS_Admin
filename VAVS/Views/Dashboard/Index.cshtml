@{
    ViewData["Title"] = "Dashboard";
}

<div class="dashboard col-lg-12">
    <section class="box nobox marginBottom0">
        <div class="content-body">
            <div class="row text-center" style="padding-bottom: 20px">

                <div class="col-md-3">
                    <div class="r4_counter db_box">
                        @*class="r4_counter db_box"*@

                        <div class="stats1">
                            <h3 class="mb-5">
                                <a href="@Url.Action("Index", "Personal")">
                                <i>
                                    <img src="~/css/img/icons8-owner-64.png" alt="">
                                </i>
                                @ViewBag.Owner    <small>people</small>

                            </h3>

                            <span>Total Owner</span>

                        </div>

                    </div>
                </div>
                <div class="col-md-3">
                    <div class="r4_counter db_box">

                        <div class="stats1">
                            <h3 class="mb-5">
                                <a href="@Url.Action("Index", "TaxValidation",new{Status="Pending"})">
                                    <i>
                                        <img src="~/css/img/icons8-data-pending-50.png" alt="">
                                    </i>
                                    @ViewBag.TotalPending <small>people</small>
                                </a>
                            </h3>
                            <span>Total Pending People</span>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="r4_counter db_box">

                        <div class="stats1">
                            <h3 class="mb-5">
                                <a href="@Url.Action("Index", "TaxValidation",new{Status="Finished"})">
                                    <i>
                                        <img src="~/css/img/icons8-production-finished-40.png" alt="">
                                    </i>
                                    @ViewBag.TotalFinish <small>people</small>
                                </a>
                               

                            </h3>
                            <span>Total Finish People</span>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="r4_counter db_box">

                        <div class="stats1">
                            <h3 class="mb-5">
                                <a href="@Url.Action("Index", "Vehicle")">
                                    <i>
                                        <img src="~/css/img/icons8-car-50.png" alt="">
                                    </i>
                                    @ViewBag.TotalVehicles  <small></small>
                                </a>
                               

                            </h3>
                            <span>Total Vehicles</span>
                        </div>
                    </div>
                </div>

            </div>
            <!-- End .row -->
        </div>
    </section>
</div>

<div class="clearfix"></div>